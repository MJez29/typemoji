{
  "name": "Typemoji",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "debugger"
  ]
}